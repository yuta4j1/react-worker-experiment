{
  "name": "react-spa-template",
  "version": "1.0.0",
  "description": "React SPA template",
  "main": "index.js",
  "author": "yuta4j1",
  "license": "MIT",
  "dependencies": {
    "firebase": "^8.2.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "serve": "^11.3.2",
    "workbox-cacheable-response": "^6.1.5",
    "workbox-strategies": "^6.1.5"
  },
  "scripts": {
    "local_dev": "webpack-dev-server",
    "serve": "webpack --mode=development && cpx manifest.json dist && cpx icon/**/*.svg dist",
    "build": "webpack --mode=production && cpx manifest.json dist && cpx icon/**/*.svg dist",
    "serve:worker": "webpack --mode=development --config webpack.worker.config.js",
    "build:worker": "webpack --mode=production --config webpack.worker.config.js",
    "start": "serve ./dist",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "test": "jest"
  },
  "devDependencies": {
    "@babel/core": "7.10.2",
    "@storybook/addon-actions": "5.3.19",
    "@storybook/addon-links": "5.3.19",
    "@storybook/addons": "5.3.19",
    "@storybook/preset-typescript": "3.0.0",
    "@storybook/react": "5.3.19",
    "@svgr/webpack": "^5.5.0",
    "@types/enzyme": "3.10.5",
    "@types/enzyme-adapter-react-16": "1.0.6",
    "@types/jest": "26.0.0",
    "@types/react": "16.9.38",
    "@types/react-dom": "16.9.8",
    "@types/styled-components": "5.1.0",
    "babel-loader": "8.1.0",
    "clean-webpack-plugin": "3.0.0",
    "cpx": "^1.5.0",
    "enzyme": "3.11.0",
    "enzyme-adapter-react-16": "1.15.2",
    "html-webpack-plugin": "4.3.0",
    "jest": "26.0.1",
    "prettier": "2.0.5",
    "styled-components": "5.1.1",
    "ts-jest": "26.1.0",
    "ts-loader": "7.0.5",
    "typescript": "3.9.5",
    "webpack": "4.43.0",
    "webpack-cli": "3.3.12",
    "webpack-dev-server": "3.11.3"
  }
}
